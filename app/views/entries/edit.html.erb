<img src=<%= @entry.image %> >
<%= form_for(@entry) do |form| %>
  <p><%= form.label("description") %></p>
  <p><%= form.text_area("description") %></p>
  <p><%= form.submit 'Edit post' %></p>


<% end %>
<p><%= link_to('Back to this entry', entry_path(@entry)) %></p>
<p><%= link_to("Back to all entries", entries_path) %></p>